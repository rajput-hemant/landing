{
  "name": "landing",
  "description": "ðŸŒ€ Personal landing page website built with QwickCity, Typescript & Tailwind CSS.",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/vercel-edge/vite.config.ts",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr",
    "dev:debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write . --cache",
    "fmt:check": "prettier --check . --cache",
    "lint": "eslint \"src/**/*.ts*\"",
    "prepare": "husky",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "type-check": "tsc --incremental --noEmit",
    "qwik": "qwik"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.12.1",
    "@builder.io/qwik-city": "^1.12.1",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@netlify/edge-functions": "^2.11.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-qwik": "^1.12.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "3.4.17",
    "typescript": "^5.7.3",
    "undici": "^7.3.0",
    "vercel": "^41.2.0",
    "vite": "^6.1.1",
    "vite-tsconfig-paths": "5.1.4"
  },
  "trustedDependencies": [
    "sharp"
  ],
  "engines": {
    "node": ">=22.14.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write --cache"
    ]
  }
}
